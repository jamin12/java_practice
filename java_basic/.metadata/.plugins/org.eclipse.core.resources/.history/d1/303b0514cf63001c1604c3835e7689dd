package data_and_operation;

public class Casting {

	public static void main(String[] args) {
		double a = 1.1;
		double b = 1;
		System.out.println(b);
		
//		int c = 1.1;
		int c = (int) 1.1;
		double d = 1.1;
		
//		1 to String
		String strI = Integer.toString(1);
		System.out.println(strI);
	}

}
